<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="unitec.ac.nz.unitecappointmentclient">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainStudentActivity" />
        <activity android:name=".MainLecturerActivity" />
        <activity android:name=".CreateAppointmentActivity" />
        <activity android:name=".StudentLecturerActivity" />
        <activity android:name=".AvailableAppointmentsActivity" />
        <activity android:name=".BookedAppointmentActivity"></activity>
    </application>

</manifest>